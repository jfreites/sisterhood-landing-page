<Layout title="Sisterhood - Terapia y consultoría psicológica.">
  <header id="top">
    <div class="mx-auto max-w-screen-xl px-4 sm:px-6 lg:px-8 md:py-6">
      <div
        class="flex flex-col md:flex-row h-40 md:h-16 items-center justify-between"
      >
        <div class="md:flex md:items-center">
          <a class="block" href="#">
            <!-- <span class="sr-only">Sisterhood</span> -->
            <h1 class="py-4 text-primary">
              <span class="text-4xl font-bold">Sisterhood</span>
            </h1>
          </a>
        </div>

        <!-- <div class="hidden md:block">
          <nav aria-label="Global">
            <ul class="flex items-center gap-6 text-md">
              <li>
                <a
                  class="text-primary transition hover:text-[#6d123f]/75"
                  href="#about"
                >
                  Sobre nosotros
                </a>
              </li>

              <li>
                <a
                  class="text-primary transition hover:text-[#6d123f]/75"
                  href="#services"
                >
